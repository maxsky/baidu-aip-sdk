{
    "name": "maxsky/baidu-aip-sdk",
    "description": "Baidu public ai php sdk",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Baidu"
        },
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Baidu\\Aip\\": "src"
        },
        "files": ["src/Helper/helper.php"]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
