{
    "name": "maxsky/baidu-aip-sdk",
    "description": "Baidu public AI php sdk",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Baidu"
        },
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Baidu\\": "src"
        },
        "classmap": [
            "src/Helper/"
        ],
        "files": [
            "src/Helper/const.php",
            "src/Helper/helper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "files": [
            "src/Helper/const.php",
            "src/Helper/helper.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
